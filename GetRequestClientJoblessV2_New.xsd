<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
           xmlns:tns="urn://rostrud/requestClientJobless/1.0.0">


    <xs:element name="response">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="status">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="result_code" minOccurs="1" maxOccurs="1">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:minLength value="1"/>
                                        <xs:maxLength value="256"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="result_description" minOccurs="1" maxOccurs="1">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:minLength value="1"/>
                                        <xs:maxLength value="2000"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="requestClientJobless" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element maxOccurs="1" minOccurs="1" name="requestID"
                                        type="string-256">
                                <xs:annotation>
                                    <xs:documentation>Идентификатор заявления в системе-отправителе</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="externalID"
                                        type="string-256">
                                <xs:annotation>
                                    <xs:documentation>Идентификатор заявления в системе-получателе</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="clientID" type="string-36">
                                <xs:annotation>
                                    <xs:documentation>Идентификатор заявителя</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="esiaID" type="string-20">
                                <xs:annotation>
                                    <xs:documentation>Идентификатор пользователя в ЕСИА</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="source">
                                <xs:annotation>
                                    <xs:documentation>Источник подачи заявления</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="PORTAL"/>
                                        <xs:enumeration value="EPGU"/>
                                        <xs:enumeration value="SZN"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="requestNum"
                                        type="string-256">
                                <xs:annotation>
                                    <xs:documentation>Номер заявления</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="requestDate"
                                        type="xs:dateTime">
                                <xs:annotation>
                                    <xs:documentation>Дата заявления, Формат: «YYYY-MM-DD Thh:mm:ss+03:00»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1"
                                        name="claim_unemployment_benefits">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель претендует на пособие по безработице
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="YES"/>
                                        <xs:enumeration value="NO"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="comment" type="string-2000" minOccurs="0"/>
                            <xs:element maxOccurs="1" minOccurs="0" name="vacancys">
                                <xs:annotation>
                                    <xs:documentation>Список подобранных вакансий</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element maxOccurs="unbounded" minOccurs="0"
                                                    name="vacance" type="vacanceType"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="archVacancys" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Архив подобранных вакансий</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element maxOccurs="unbounded" minOccurs="0"
                                                    name="archVacance" type="vacanceType"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>

                            <xs:element name="firstName" type="string-256"/>
                            <xs:element name="lastName" type="string-256"/>
                            <xs:element name="middleName" type="string-256"
                                        minOccurs="0"/>

                            <xs:element name="birthDate" type="xs:date"/>
                            <xs:element name="gender">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="MALE"/>
                                        <xs:enumeration value="FEMALE"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="personCitizenship" type="string-256"></xs:element>
                            <xs:element name="passportSeries">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:length value="4"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="passportNumber">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:length value="6"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="issueDate" type="xs:date"/>
                            <xs:element name="issuedBy" type="string-2000"/>
                            <xs:element name="personINN" type="INNType"/>
                            <xs:element name="personSNILS" type="SNILSType"/>

                            <xs:element name="addressType">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="CONSTANT"/>
                                        <xs:enumeration value="TEMPORARY"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="stateRegionCode">
                                <xs:annotation>
                                    <xs:documentation>Код региона места жительства (пребывания) по КЛАДР
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="RegionCodeType">
                                        <xs:pattern value="\d{13}"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="homeCityCode">
                                <xs:annotation>
                                    <xs:documentation>Район, населенный пункт, улица места жительства
                                        (пребывания) по КЛАДР
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="homeAddressFiasId" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Глобальный уникальный номер адресообразующего элемента
                                        места жительства (пребывания) в государственном адресном реестре
                                        ФИАС
                                    </xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:length value="36"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="addressRegion" type="string-256">
                                <xs:annotation>
                                    <xs:documentation>Регион места жительства (пребывания)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="addressCity" type="string-256">
                                <xs:annotation>
                                    <xs:documentation>Район, населенный пункт, улица места жительства
                                        (пребывания)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="addressHouse" type="string-256">
                                <xs:annotation>
                                    <xs:documentation>Номер дома, корпуса, строения места жительства
                                        (пребывания)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="addressFlat" type="string-256"
                                        minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Номер квартиры места жительства (пребывания)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>

                            <xs:element name="personEMail" type="string-2000">
                                <xs:annotation>
                                    <xs:documentation>Адрес электронной почты</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="personMobilePhone">
                                <xs:annotation>
                                    <xs:documentation>Номер мобильного телефона</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:length value="11"/>
                                        <xs:pattern value="\d{11}"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="priorityCommunication" type="string-256"
                                        minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Приоритетный способ связи</xs:documentation>
                                </xs:annotation>
                            </xs:element>


                            <xs:element name="submitRegion" type="string-256">
                                <xs:annotation>
                                    <xs:documentation>Регион обращения</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="submitOfficeId" type="string-2000">
                                <xs:annotation>
                                    <xs:documentation>Код Центра занятости населения (множественный
                                        элемент)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="submitOfficeName" type="string-2000">
                                <xs:annotation>
                                    <xs:documentation>Наименование Центра занятости населения
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>

                            <xs:element name="job_search_regions" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Регионы поиска работы</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>

                                        <xs:element name="job_search_regionName"
                                                    type="string-256">
                                            <xs:annotation>
                                                <xs:documentation>Наименование региона поиска работы
                                                </xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="job_search_regionCode"
                                                    type="string-256">
                                            <xs:annotation>
                                                <xs:documentation>Код региона поиска работы
                                                </xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="isApplicantPrepensioner" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак отношения заявителя к гражданам
                                        предпенсионного возраста
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isApplicantHandicapped" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак наличия у заявителя инвалидности
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isApplicantOrphan" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак отношения заявителя к детям-сиротам, детям,
                                        оставшимся без попечения родителей
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isApplicantHaveChildren" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак наличия у заявителя ребенка (детей) в возрасте
                                        до 18 лет
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isApplicantGuardianChildren"
                                        type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Являюсь опекуном, усыновителем ребенка (детей) в
                                        возрасте до 18 лет
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isApplicantHaveChildrenForeign"
                                        type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Имею ребенка (детей), зарегистрированного компетентным
                                        органом иностранного государства
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isApplicantPensioner" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель является пенсионером
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isApplicantMilitaryPensioner"
                                        type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель является военным
                                        пенсионером
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isLiberated" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель является освобожденным из
                                        мест лишения свободы
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isRefugee" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель является
                                        беженцем/вынужденным переселенцем
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isDisabledParent" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель является
                                        одиноким/многодетным родителем
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isFiredMilitary" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель является уволенным с
                                        военной службы/членом его семьи
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="isExposureRadiation" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель является подвергшимся
                                        воздействию радиации вследствие чернобыльской и других радиационных
                                        аварий и катастроф
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="is1618" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель является
                                        несовершеннолетним в возрасте от 16 до 18 лет
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="is1825" type="xs:boolean">
                                <xs:annotation>
                                    <xs:documentation>Признак того, что Заявитель имеет среднее
                                        профессиональное образование или высшее образование и ищущим работу
                                        в течение года с даты выдачи документа об образовании и о
                                        квалификации (в возрасте от 18 до 25 лет)
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1"
                                        name="isApplicantPensioner2" type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Получающие пенсию по старости или за выслугу лет»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="isLessThan16"
                                        type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Не достигшие 16 летнего возраста»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="isPrisoner"
                                        type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Осужденные по решению суда к исправительным работам, а
                                        также к наказанию в виде лишения свободы»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1"
                                        name="isPaidPosition" type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Избранные, назначенные или утвержденные на оплачиваемую
                                        должность»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1"
                                        name="isWorkedOnContracts" type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Выполняющие работы по договорам»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1"
                                        name="isAncillaryCraft" type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Занятые в подсобных промыслах и реализующих продукцию по
                                        договорам»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="havePayingJob"
                                        type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории "Выполняющие оплачиваемую работу"
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1"
                                        name="isEntrepreneur" type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Индивидуальные предприниматели, нотариусы, адвокаты»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1"
                                        name="isMilitaryPersonnel" type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Проходящие военную или альтернативную гражданскую службу»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="isStudent"
                                        type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Обучающиеся по очной форме обучения»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="isFounder"
                                        type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Учредители организаций (кроме некоммерческих без права
                                        получения дохода от деятельности)»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1"
                                        name="isTemporarilyAbsent" type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Временно отсутствующие на рабочем месте»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="isMemberKFH"
                                        type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Член крестьянского (фермерского) хозяйства»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="isPayerNPD"
                                        type="string-1">
                                <xs:annotation>
                                    <xs:documentation>Признак подтверждения Заявителем его принадлежности к
                                        категории «Плательщик налога на профессиональный доход»
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>

                               <xs:element name="receiveMethod" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="receiveMethod" type="string-256"
                                                    minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Способ получения пособия. Возможные значения:
                                                    1 – расчетный счет;
                                                    2 – почтовый перевод;
                                                    3 - карта "МИР".
                                                </xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="receiveMethodBank" minOccurs="0">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="bankINN" minOccurs="0">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:length value="10"/>
                                                                <xs:pattern value="\d{10}"/>
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="bankName" type="string-256"/>
                                                    <xs:element name="bankBIC">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:length value="9"/>
                                                                <xs:pattern value="\d{9}"/>
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="bankCorrAccount">
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:string">
                                                                <xs:length value="20"/>
                                                                <xs:pattern value="\d{20}"/>
                                                            </xs:restriction>
